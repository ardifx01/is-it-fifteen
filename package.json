{
    "name": "@is-it/fifteen",
    "version": "1.0.1",
    "type": "module",
    "description": "A simple package to check if a number is 15",
    "main": "dist/index.js",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "lint": "eslint .",
        "preview": "vite preview",
        "format": "prettier --write .",
        "build:pkg": "bun build ./package/*.ts --outdir ./dist --target=node --minify --root package && tsc --emitDeclarationOnly --project tsconfig.pkg.json",
        "npm": "bun build:pkg && bun publish"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kimmyxpow/is-it-fifteen.git"
    },
    "keywords": [
        "check",
        "number",
        "fifteen",
        "typescript"
    ],
    "author": "kimmyxpow",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/kimmyxpow/is-it-fifteen/issues"
    },
    "homepage": "https://github.com/kimmyxpow/is-it-fifteen#readme",
    "dependencies": {
        "@react-three/fiber": "^8.17.10",
        "@tailwindcss/typography": "^0.5.15",
        "@tailwindcss/vite": "^4.0.0-beta.8",
        "@types/three": "^0.171.0",
        "clsx": "^2.1.1",
        "framer-motion": "^11.15.0",
        "motion": "^11.15.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-syntax-highlighter": "^15.6.1",
        "tailwind-merge": "^2.6.0",
        "tailwindcss": "^4.0.0-beta.8",
        "three": "^0.172.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.17.0",
        "@iconify/react": "^5.2.0",
        "@types/react": "^18.3.18",
        "@types/react-dom": "^18.3.5",
        "@types/react-syntax-highlighter": "^15.5.13",
        "@vitejs/plugin-react-swc": "^3.5.0",
        "bun-types": "^1.1.42",
        "eslint": "^9.17.0",
        "eslint-plugin-react-hooks": "^5.0.0",
        "eslint-plugin-react-refresh": "^0.4.16",
        "globals": "^15.14.0",
        "prettier": "^3.4.2",
        "prettier-plugin-tailwindcss": "^0.6.9",
        "typescript": "~5.6.2",
        "typescript-eslint": "^8.18.2",
        "vite": "^6.0.5",
        "vite-tsconfig-paths": "^5.1.4"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist"
    ]
}
